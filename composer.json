{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "doctrine/dbal": "^2.0",
        "fideloper/proxy": "^4.4",
        "graham-campbell/github": "^9.4",
        "imdbphp/imdbphp": "^6.4",
        "mcamara/laravel-localization": "^1.2",
        "laravel/framework": "^6.20",
        "php-http/guzzle6-adapter": "^1.1",
        "polakosz/mafab": "^1.1"
    },
    "require-dev": {
        "facade/ignition": "^1.16.4",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.5.8",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "run-all-tests": ".\\vendor\\bin\\phpunit --bootstrap .\\vendor\\autoload.php --testdox .\\tests\\",
        "get-coverage": ".\\vendor\\bin\\phpunit --bootstrap .\\vendor\\autoload.php --coverage-html code-cov .\\tests\\",
        "run-tidy": ".\\vendor\\bin\\phpcbf --extensions=php ./app --standard=PSR2 && .\\vendor\\bin\\phpcbf --extensions=php ./tests --standard=PSR2",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
